<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SignBuzz.Game1.GameOnePage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="GameOnePage!"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button Clicked="takePicture" Text="Press here to take a picture"></Button>
            <Button Text="Game One is on!" Clicked="OnNextPageButtonClicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>